<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pq.eventbusdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".eventbus2.E2ParentActivity"/>

        <activity android:name=".eventbus2.E2MainActivity"/>

        <activity android:name=".eventbus2.E2SendActivity"/>


        <activity android:name=".eventbus3.E3ParentActivity"/>

        <activity android:name=".eventbus3.E3MainActivity"/>

        <activity android:name=".eventbus3.E3SendActivity"/>
    </application>

</manifest>